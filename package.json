{
  "name": "create-vv-app",
  "version": "0.0.1",
  "description": "A Veve App with unocss and autoimport",
  "productName": "Veve App",
  "homepage": "https://github.com/vevedh/create-vv-app",
  "author": "vevedh <vevedh@gmail.com>",
  "type": "module",
  "private": true,
  "feathers": {
    "language": "js",
    "packager": "bun",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": false
  },
  "directories": {
    "lib": "api",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "dev": "dotenvx run -f .env -- quasar dev",
    "backend:docker": "dotenvx run -f .env -- nodemon --watch ./api --watch ./dist --watch ./package.json ./api",
    "dev:backend": "dotenvx run -f .env -- docker compose  -f \"docker-compose-prod.yml\" up -d --build backend",
    "dev:mongodb": "dotenvx run -f .env -- docker compose  -f \"docker-compose-mongodb.yml\" up -d --build mongo",
    "dev:docker": "dotenvx run -f .env -- quasar build && docker compose  -f \"docker-compose-prod.yml\" up -d",
    "dev:kill:mongodb": "dotenvx run -f .env -- docker kill create-vv-app-mongo-1",
    "dev:icons": "icongenie generate -i src/assets/fbavatar.png",
    "gen:env": "phasme generate env",
    "build": "dotenvx run -f .env -- quasar build"
  },
  "dependencies": {
    "@blocksuite/blocks": "^0.16.0-canary-202407191506-aa26eda",
    "@blocksuite/presets": "^0.16.0-canary-202407191506-aa26eda",
    "@blocksuite/store": "^0.16.0-canary-202407191506-aa26eda",
    "@dotenvx/dotenvx": "^1.6.4",
    "@featherscloud/pinion": "^0.5.4",
    "@feathersjs/authentication-client": "^5.0.29",
    "@feathersjs/authentication-local": "^5.0.29",
    "@feathersjs/commons": "^5.0.29",
    "@feathersjs/configuration": "^5.0.29",
    "@feathersjs/errors": "^5.0.29",
    "@feathersjs/express": "^5.0.29",
    "@feathersjs/feathers": "^5.0.29",
    "@feathersjs/mongodb": "^5.0.29",
    "@feathersjs/rest-client": "^5.0.29",
    "@feathersjs/socketio": "^5.0.29",
    "@feathersjs/socketio-client": "^5.0.29",
    "@feathersjs/transport-commons": "^5.0.29",
    "@phasme/cli": "^1.0.40",
    "@quasar/cli": "^2.4.1",
    "@quasar/extras": "^1.16.12",
    "@quasar/icongenie": "^4.0.0",
    "@vueuse/motion": "^2.2.3",
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "connect-history-api-fallback": "^2.0.0",
    "date-fns": "^3.6.0",
    "feathers-pinia": "^4.5.4",
    "feathers-swagger": "^3.0.0",
    "gray-matter": "^4.0.3",
    "kill-port": "^2.0.1",
    "nodemon": "^3.1.4",
    "pinia": "^2.1.7",
    "quasar": "^2.16.6",
    "remove-markdown": "^0.5.0",
    "shx": "^0.3.4",
    "socket.io-client": "^4.7.5",
    "vitepress": "^1.3.1",
    "vue": "^3.4.33",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "vv-feathers-mongodb-management": "^2.1.5"
  },
  "devDependencies": {
    "@eslym/vite-plugin-express-app": "^1.0.2",
    "@iconify-json/carbon": "^1.1.36",
    "@iconify-json/ion": "^1.1.18",
    "@iconify-json/logos": "^1.1.43",
    "@iconify-json/mdi": "^1.1.67",
    "@iconify-json/simple-icons": "^1.1.109",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@quasar/app-vite": "^2.0.0-beta.15",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/cli": "^0.61.5",
    "@unocss/reset": "^0.61.5",
    "@vite-pwa/vitepress": "^0.5.0",
    "@vueuse/core": "^10.11.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "feathers-vite": "^1.2.7",
    "prettier": "^3.3.3",
    "typescript": "~5.3.3",
    "unocss": "^0.61.5",
    "unplugin-auto-import": "^0.17.8",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-express": "^1.0.3",
    "vite3-plugin-express": "^0.1.5",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}